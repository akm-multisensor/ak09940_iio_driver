// Definitions for AK5704 ADC
/dts-v1/;
/plugin/;

/ {
	compatible = "brcm,bcm2708";

	fragment@1 {
		target = <&i2c1>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			ak09940@10 {
				#sound-dai-cells = <0>;
				compatible = "asahi-kasei,ak09940";
				reg = <0x10>;
				ak09940,pdn-gpio = <&gpio 4 0>;
				status = "okay";
			};
		};

	};

};
 
